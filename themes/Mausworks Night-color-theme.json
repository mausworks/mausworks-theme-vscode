    /* JSX */
    {
      "name": "JSX text children",
      "scope": ["meta.jsx.children"],
      "settings": {
        "foreground": "#effffde9"
      }
    },
    /* Markdown */
    {
      "name": "Markdown bold",
      "scope": ["markup.heading.markdown", "markup.bold.markdown"],
      "settings": {
        "foreground": "#effffde9",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown tags",
      "scope": [
        "punctuation.definition.heading.markdown",
        "punctuation.definition.bold.markdown",
        "punctuation.definition.italic.markdown",
        "punctuation.definition.raw.markdown",
        "punctuation.definition.string.begin.markdown",
        "punctuation.definition.string.end.markdown",
        "markup.fenced_code.block.markdown"
      ],
      "settings": {
        "foreground": "#effffd57",
        "fontStyle": ""
      }
    },
    {
      "name": "Markdown italic",
      "scope": ["markup.italic.markdown", "meta.link.inline.markdown"],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Inserted",
      "scope": ["markup.inserted"],
      "settings": {
        "foreground": "#48F9BB"
      }
    },
    {
      "name": "Deleted",
      "scope": ["markup.deleted"],
      "settings": {
        "foreground": "#D54D57"
      }
    },
    {
      "name": "Changed",
      "scope": ["markup.changed"],
      "settings": {
        "foreground": "#5EE8D9"
      }
    },
    {
      "name": "Regular Expressions",
      "scope": ["string.regexp"],
      "settings": {
        "foreground": "#FDFD9B"
      }
    },
    {
      "name": "Escape Characters",
      "scope": ["constant.character.escape"],
      "settings": {
        "foreground": "#effffd57"
      }
    },
    {
      "name": "URL",
      "scope": ["*url*", "*link*", "*uri*"],
      "settings": {
        "fontStyle": "underline"
      }
    }
  ]
}
