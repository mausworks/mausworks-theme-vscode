  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#effffd38"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "string constant.other.placeholder",
        "meta.function-call.arguments",
        "meta.object.member",
        "meta.directive"
      ],
      "settings": {
        "foreground": "#effffd"
      }
    },
    {
      "name": "Function call parenthesis",
      "scope": [
        "punctuation.definition.arguments.begin",
        "punctuation.definition.arguments.end"
      ],
      "settings": {
        "foreground": "#effffd"
      }
    },
    {
      "name": "Colors",
      "scope": ["constant.other.color"],
      "settings": {
        "foreground": "#effffd"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#BF0D24"
      }
    },
    {
      "name": "Operator, Misc",
      "scope": [
        "keyword.control",
        "constant.other.color",
        "punctuation.separator.inheritance.php",
        "punctuation.section.embedded",
        "keyword.other.template",
        "keyword.other.substitution"
      ],
      "settings": {
        "foreground": "#effffd58"
      }
    },
    {
      "name": "HTML brackets/braces",
      "scope": [
        "punctuation.definition.tag.html",
        "punctuation.definition.tag.begin",
        "punctuation.definition.tag.end"
      ],
      "settings": {
        "foreground": "#effffd58"
      }
    },
    {
      "name": "Tag",
      "scope": [
        "entity.name.tag",
        "meta.tag.sgml",
        "markup.deleted.git_gutter"
      ],
      "settings": {
        "foreground": "#effffde6"
      }
    },
    {
      "name": "Function, Special Method",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "variable.function",
        "support.function",
        "keyword.other.special-method"
      ],
      "settings": {
        "foreground": "#FF1F93"
      }
    },
    {
      "name": "Macros",
      "scope": "entity.name.function.macro",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Operators",
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#effffd"
      }
    },
    {
      "name": "New operator",
      "scope": "keyword.operator.new",
      "settings": {
        "foreground": "#effffd57"
      }
    },
    {
      "name": "Try, catch and throw",
      "scope": "keyword.control.trycatch",
      "settings": {
        "foreground": "#effffd57",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Punctuation accessor (the dot in foo.bar)",
      "scope": "punctuation.accessor",
      "settings": {
        "foreground": "#effffd"
      }
    },
    {
      "name": "Support constant (e.g. JSON, in JSON.stringify)",
      "scope": "support.constant",
      "settings": {
        "foreground": "#48F9BB"
      }
    },
    {
      "name": "Language Constant",
      "scope": ["constant.language"],
      "settings": {
        "foreground": "#effffd57",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Number, Constant, Tag Attribute, Embedded",
      "scope": ["constant.numeric", "constant.character", "constant.escape"],
      "settings": {
        "foreground": "#FDFD9B"
      }
    },
    {
      "name": "Keywords, Modifiers",
      "scope": [
        "keyword.other.unit",
        "keyword.other",
        "keyword",
        "storage.modifier",
        "storage.type"
      ],
      "settings": {
        "foreground": "#effffd52"
      }
    },
    {
      "name": "Super and self variables",
      "scope": ["variable.language.super", "variable.language.self"],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Mutable storage modifier (in e.g Rust)",
      "scope": "storage.modifier.mut",
      "settings": {
        "foreground": "#5EE8D9",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Attribute brackets and definition (in e.g Rust)",
      "scope": [
        "punctuation.definition.attribute",
        "meta.attribute punctuation.brackets"
      ],
      "settings": {
        "foreground": "#effffd52"
      }
    },
    {
      "name": "Relations (for e.g Databases)",
      "scope": "variable.language.relations",
      "settings": {
        "foreground": "#48F9BB",
        "fontStyle": "italic"
      }
    },
    {
      "name": "String, Symbols, Markup Heading",
      "scope": [
        "string",
        "constant.other.symbol",
        "constant.other.key",
        "markup.heading",
        "markup.inserted.git_gutter",
        "meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
      ],
      "settings": {
        "foreground": "#FDFD9B"
      }
    },
    {
      "name": "Class, Support",
      "scope": [
        "entity.name.type",
        "support.type",
        "support.class",
        "meta.use.php",
        "entity.other.inherited-class",
        "support.other.namespace.use.php",
        "support.other.namespace.php",
        "markup.changed.git_gutter",
        "support.type.sys-types"
      ],
      "settings": {
        "foreground": "#48F9BB"
      }
    },
    {
      "name": "Attributes",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#effffde9"
      }
    },
    {
      "name": "HTML Attributes",
      "scope": [
        "text.html.basic entity.other.attribute-name.html",
        "text.html.basic entity.other.attribute-name"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#FFCB6B"
      }
    },
    /* StyleSheets (CSS, SASS, Stylus ...) */
    {
      "name": "CSS Tag Selector",
      "scope": [
        "source.css entity.name.tag",
        "source.sass entity.name.tag",
        "source.scss entity.name.tag",
        "source.less entity.name.tag",
        "source.stylus entity.name.tag",
        "source.postcss entity.name.tag"
      ],
      "settings": {
        "foreground": "#48F9BB",
        "fontStyle": "italic"
      }
    },
    {
      "name": "CSS Class Selector",
      "scope": ["entity.other.attribute-name.class"],
      "settings": {
        "foreground": "#11C3D0"
      }
    },
    {
      "name": "CSS ID Selector",
      "scope": ["entity.other.attribute-name.id"],
      "settings": {
        "foreground": "#FF1F93"
      }
    },
    {
      "name": "CSS Pseudo Selector",
      "scope": [
        "entity.other.attribute-name.pseudo-element",
        "entity.other.attribute-name.pseudo-class"
      ],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "CSS Property Names",
      "scope": [
        "source.css support.type.property-name",
        "source.sass support.type.property-name",
        "source.scss support.type.property-name",
        "source.less support.type.property-name",
        "source.stylus support.type.property-name",
        "source.postcss support.type.property-name"
      ],
      "settings": {
        "foreground": "#effffd"
      }
    },
    {
      "name": "CSS Property Values",
      "scope": [
        "source.css support.constant.property-value",
        "source.sass support.constant.property-value",
        "source.scss support.constant.property-value",
        "source.less support.constant.property-value",
        "source.stylus support.constant.property-value",
        "source.postcss support.constant.property-value"
      ],
      "settings": {
        "foreground": "#FDFD9B"
      }
    },
    {
      "name": "CSS Units",
      "scope": ["keyword.other.unit"],
      "settings": {
        "foreground": "#effffd80"
      }
    },
    {
      "name": "CSS colors",
      "scope": "constant.other.color",
      "settings": {
        "foreground": "#FDFD9B"
      }
    },
    {
      "name": "CSS Braces",
      "scope": [
        "punctuation.section.property-list.begin.bracket.curly",
        "punctuation.section.property-list.end.bracket.curly",
        "punctuation.terminator.rule"
      ],
      "settings": {
        "foreground": "#effffd58"
      }
